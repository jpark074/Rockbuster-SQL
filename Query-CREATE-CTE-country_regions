--A query utilizing the Create statement and CTE to create a table that contains country_id, country and region each country belongs to

--DROP TABLE country_regions;					
					
CREATE TABLE country_regions (					
	country_id SMALLINT,				
	country VARCHAR(100),				
	region VARCHAR(50),				
	PRIMARY KEY(country_id)				
);					
					
